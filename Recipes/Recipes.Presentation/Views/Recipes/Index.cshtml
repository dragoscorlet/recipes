    <script src="@Url.Content("~/Scripts/RecipeDetails.js")"></script>


    <div class="search-container" id="autocomplete">
        <input type="text"  id="search"  placeholder="Cauta ingrediente">
        <div id="selectedIngredients"></div>
        </div>


    <div id="recipes">
    </div>

    <div id="recipe" class="container">
    </div>

    <input type="hidden" id="page" value="0"/>
 

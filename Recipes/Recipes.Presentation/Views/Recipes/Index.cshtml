    <link href="@Url.Content("~/CSS/Autocomplete.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/RecipeListings.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/CSS/RecipeDetails.css")" rel="stylesheet" type="text/css" />
    <title>Recipes</title>
</head>
<body>
    <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
    <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
